<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="kr.campus.mapper.AdminMapper">
  
     <select id="getList" resultType="kr.campus.domain.ItemVO">
   <![CDATA[    
   select *from tbl_board where bno > 0
   ]]>   </select>

  
  
  <insert id="itemsEnroll">
     
        insert into tbl_items(itemCode,itemName,price,quantity,category,thumbNail,details)
      values(
      #{itemcode},
      #{itemname},
      #{price},
      #{quantity},
      #{category},
      #{thumbnail},
      #{details}
      )

     </insert>
  
  </mapper>